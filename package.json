{
  "name": "yuthiesaari",
  "version": "1.0.0",
  "description": "yuthiesaari website",
  "main": "server.js",
  "engines": {
    "node": "12.18.3",
    "npm": "6.14.6"
  },
  "scripts": {
    "start": "node backend/server",
    "server": "nodemon backend/server",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder",
    "data:destroy": "node backend/seeder -d",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && npm run build --prefix frontend && npm run dev"
  },
  "author": "Yuwetha Nesan",
  "license": "MIT",
  "dependencies": {
    "@trendmicro/react-sidenav": "^0.5.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "infinite-react-carousel": "^1.2.11",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.7",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.14",
    "owl-carousel": "^1.0.0",
    "paypal-rest-sdk": "^1.8.1",
    "react": "^17.0.1",
    "react-alice-carousel": "^2.0.1",
    "react-owl-carousel": "^2.3.1",
    "react-responsive-carousel": "^3.2.10",
    "react-slick": "^0.27.12"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.4"
  }
}
